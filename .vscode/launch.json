{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Swagger",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/MuslimSalat.API/bin/Debug/net9.0/MuslimSalat.API.dll",
        "args": [],
        "cwd": "${workspaceFolder}/MuslimSalat.API",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
            "uriFormat": "%s/swagger/"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
      }  
    ]
}